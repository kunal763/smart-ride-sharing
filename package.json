{
  "name": "airport-ride-pooling",
  "version": "1.0.0",
  "description": "Smart Airport Ride Pooling Backend System",
  "main": "dist/index.js",
  "scripts": {
    "dev": "node --max-old-space-size=4096 node_modules/.bin/ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node --max-old-space-size=4096 dist/index.js",
    "migrate": "ts-node src/database/migrate.ts",
    "seed": "ts-node src/database/seed.ts",
    "seed:10k": "node tests/setup/seed-10k-users.js",
    "seed:cabs": "node tests/setup/seed-cabs.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "clear-pools": "bash tests/setup/clear-pools.sh",
    "setup-test": "bash tests/setup/setup-autocannon-test.sh",
    "load-test": "node tests/load-tests/load-test-autocannon.js",
    "load-test:full": "node tests/load-tests/load-test-100rps-full.js",
    "test:api": "node tests/api-tests/test-api.js",
    "test:matching": "node tests/api-tests/test-matching.js",
    "test:pricing": "node tests/api-tests/test-pricing.js",
    "test:user-cab": "node tests/api-tests/test-user-cab-api.js",
    "test:cab-location": "node tests/api-tests/test-cab-location-update.js",
    "confirm": "node tests/api-tests/confirm-ride.js"
  },
  "keywords": [
    "ride-pooling",
    "airport",
    "optimization"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bull": "^4.11.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "autocannon": "^8.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
